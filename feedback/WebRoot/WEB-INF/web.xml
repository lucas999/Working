<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>ssss</servlet-name>
    <servlet-class>org.bestton.demo.ssss</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>demo1</servlet-name>
    <servlet-class>org.bestton.servletDemo.demo1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>demo2</servlet-name>
    <servlet-class>org.bestton.servletDemo.demo2</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>ssss</servlet-name>
    <url-pattern>/servlet/ssss</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>demo1</servlet-name>
    <url-pattern>/servlet/demo1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>demo2</servlet-name>
    <url-pattern>/servlet/demo2</url-pattern>
  </servlet-mapping>

</web-app>